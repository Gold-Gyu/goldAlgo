   "source": [
    "# 어떤 특정 문자가 연속으로 나올 때 누적합\n",
    "# 연속해서 나오면 \n",
    "TC = int(input())\n",
    "ans = 0\n",
    "for i in range(1, TC+1):\n",
    "    OX = input()\n",
    "    if OX[i-1] == OX[i]:\n",
    "        ans += 1\n",
    "        print(OX.count(\"o\"))\n",
    "\n",
    "print(ans)"
   ]
  }
